<article class="post">
  <div class="post-title">
    <h2 class="title"><%= page.title %></h2>
  </div>
   <div class="post-meta">
    <span class="post-time"><%- date(page.date, "YYYY-MM-DD") %></span>
  </div>

          <strong class="toc-title">文章目录</strong> 
          <%- toc(page.content, {list_number: false}) %>

  <div class="post-content">

    <%- page.content %>
  </div>
</article> 


